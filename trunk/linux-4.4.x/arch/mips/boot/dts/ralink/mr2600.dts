/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-spi-16m-layout.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "Motorola MR2600";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		power_orange {
			label = "power_2";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
		};

		power_green {
			label = "power";
			gpios = <&gpio0 16 GPIO_ACTIVE_LOW>;
		};

		net_orange {
			label = "wan_2";
			gpios = <&gpio0 4 GPIO_ACTIVE_LOW>;
		};

		net_green {
			label = "wan";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
		};

		usb1 {
			label = "usb_1";
			gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		fn1 { /* wlan */
			label = "fn1";
			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_FN_1>;
		};
	};
};

&raeth {
	mac-address = [00 00 00 00 00 00];
	gmac1-support = "rgmii-1";
	rgmii-mode-1 = "force-1000";
	gmac2-support = "rgmii-2";
	rgmii-mode-2 = "force-1000";
	gmac2-phy-address = <0>;
	gmac1_txq_num = <1>;
	gmac1_txq_txd_num = <1024>;
	gmac2_txq_num = <1>;
	gmac2_txq_txd_num = <1024>;
	num_rx_desc = <1024>;
	status = "okay";
};

&pcie {
	status = "okay";
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			mtk,group = "i2c", "uart2", "uart3";
			mtk,function = "gpio";
		};
	};
};

&xhci {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};
