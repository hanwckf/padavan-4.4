/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "ZTE E8820S";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";
    
    		sys_white {
			label = "power";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
		};
	};
	
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
		
		wps {
			label = "wps";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
		
		wifi {
			label = "wifi";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RFKILL>;
		};
	};
	
};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0 0x80000>; /* 512k */
			};
						
			partition@80000 {
				label = "Config";
				reg = <0x80000 0x80000>; /* 512k */
			};

			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x40000>; /* 256k */
			};

			partition@140000 {
				label = "firmware";
				reg = <0x140000 0x2000000>; /* 32M */
			};
			
			partition@2140000 {
				label = "Storage";
				reg = <0x2140000 0x400000>; /* 4096K */
			};

			partition@2540000 {
				label = "RWFS";
				reg = <0x2540000 0x5A40000>;
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&xhci {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};
